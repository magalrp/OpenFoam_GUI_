FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.05;

boundaryField
{

    INLET_Secondary_flow
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.05;
        value           uniform 0.1;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.1;
    }

    walls
    {
        type            omegaWallFunction;
        value           uniform 5;
    }

}

